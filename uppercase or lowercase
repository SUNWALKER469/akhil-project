//WAP to find whether the character is uppercase or lowercase
#include<stdio.h>

int main()
{
	char ch;
	printf("Akhil Hamal\n");
	printf("Enter a character:");
	scanf("%c", &ch);
	
	//for upper case
	if(ch >='A' && ch<='Z'){
		printf("upper case");
	}
	
	//for lower case	
	else if(ch >='a' && ch<='z'){
		printf("lower case");
	} 
	else {
		printf("not a english letter\n");
	}
	return 0;
}
		
		
